    Private Sub Keyboard_Load(sender As Object, e As EventArgs) Handles MyBase.Load
        TextBoxKeyboard.Text = LeaderConfirmDefect.tbEmpCodeleader.Text
        TextBoxKeyboard.Enabled = False
